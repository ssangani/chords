  "description": "Template API repo",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "test": "jest",
    "prepare": "husky",
    "lint": "eslint src --fix",
    "prestart": "npm run build",
    "start": "node .",
    "format": "prettier src --write"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
